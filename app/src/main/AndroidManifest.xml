<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="pl.brainfuck.humanoid">

    <uses-feature
        android:glEsVersion="0x00020000"
        android:required="true" />

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/humanoid"
        android:label="@string/app_name"
        android:roundIcon="@drawable/humanoid"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="pl.brainfuck.humanoid.MenuActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="pl.brainfuck.humanoid.PlayActivity"></activity>
        <activity android:name="pl.brainfuck.humanoid.PrefActivity"></activity>
    </application>

</manifest>